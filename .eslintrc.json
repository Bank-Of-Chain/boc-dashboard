{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "CHAIN_BROWSER_URL": true,
    "IMAGE_ROOT": true,
    "DASHBOARD_ROOT": true,
    "API_SERVER": true,
    "ENV_INDEX": true,
    "USDI": true,
    "ETHI": true,
    "RPC_URL": true
  }
}