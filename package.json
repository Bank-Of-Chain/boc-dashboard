{
  "name": "boc-dashboard",
  "homepage": "https://bank-of-chain.github.io/boc-dashboard",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "apollo": "node ./config/apollo.js",
    "mustache": "mustache ./config/address.json ./config/config.template.js > ./config/config.js",
    "start": "umi dev",
    "build": "umi build",
    "apollo:local": "node config/apollo.js --env=local",
    "apollo:qa-sg": "node config/apollo.js --env=qa-sg",
    "apollo:qa02-sg": "node config/apollo.js --env=qa02-sg",
    "apollo:qa03-sg": "node config/apollo.js --env=qa03-sg",
    "apollo:qa04-sg": "node config/apollo.js --env=qa04-sg",
    "apollo:stage-sg": "node config/apollo.js --env=stage-sg",
    "apollo:pr-sg": "node config/apollo.js --env=pr-sg",
    "apollo:pr02-sg": "node config/apollo.js --env=pr02-sg",
    "analyze": "cross-env ANALYZE=1 umi build",
    "dev": "npm run start:local",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --write .",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "prettier": "prettier -c -w \"src/**/*.{js,jsx}\"",
    "serve": "umi-serve",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=local umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=local umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "depcheck": "depcheck",
    "prepare": "husky install",
    "unimported": "unimported --flow"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^0.9.4",
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-layout": "^6.15.3",
    "@apollo/client": "^3.5.7",
    "@gnosis.pm/safe-apps-sdk": "^7.0.0",
    "@gnosis.pm/safe-apps-web3modal": "^9.0.0",
    "@walletconnect/web3-provider": "^1.7.7",
    "antd": "^4.14.0",
    "bignumber.js": "^9.0.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.2",
    "ethers": "^5.5.4",
    "graphql": "^16.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-responsive": "^9.0.0-beta.6",
    "umi": "^3.5.0",
    "umi-serve": "^1.9.10",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@ethersproject/providers": "^5.6.8",
    "@loadable/component": "^5.15.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^17.0.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/openapi": "^1.1.14",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-openapi": "^1.2.0",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-dumi": "^1.1.7",
    "@umijs/preset-react": "^1.8.17",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/yorkie": "^2.0.3",
    "axios": "^0.27.2",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "depcheck": "^1.4.3",
    "detect-installer": "^1.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "filemanager-webpack-plugin": "^7.0.0",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.0",
    "inquirer": "8.2.0",
    "jest-environment-node": "^28.1.3",
    "lint-staged": "^10.0.0",
    "minimist": "^1.2.6",
    "mockjs": "^1.0.1-beta3",
    "mustache": "^4.2.0",
    "prettier": "^2.3.2",
    "prop-types": "^15.8.1",
    "puppeteer": "^16.1.0",
    "puppeteer-core": "^8.0.0",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2",
    "unimported": "^1.21.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}