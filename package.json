{
  "name": "boc-dashboard",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "apollo": "node ./config/apollo.js",
    "mustache": "mustache ./config/address.json ./config/config.template.js > ./src/config/config.js",
    "start": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "apollo:local": "node config/apollo.js --env=local",
    "apollo:qa-sg": "node config/apollo.js --env=qa-sg",
    "apollo:qa02-sg": "node config/apollo.js --env=qa02-sg",
    "apollo:qa03-sg": "node config/apollo.js --env=qa03-sg",
    "apollo:qa04-sg": "node config/apollo.js --env=qa04-sg",
    "apollo:qa05-sg": "node config/apollo.js --env=qa05-sg",
    "apollo:stage-sg": "node config/apollo.js --env=stage-sg",
    "apollo:pr-sg": "node config/apollo.js --env=pr-sg",
    "apollo:pr02-sg": "node config/apollo.js --env=pr02-sg",
    "dev": "npm run start:local",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier -c -w \"src/**/*.{js,jsx}\"",
    "pretest": "node ./tests/beforeTest",
    "test:all": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit",
    "depcheck": "depcheck",
    "prepare": "husky install",
    "unimported": "unimported --flow"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@apollo/client": "^3.5.7",
    "@gnosis.pm/safe-apps-sdk": "^7.0.0",
    "@gnosis.pm/safe-apps-web3modal": "^9.0.0",
    "@walletconnect/web3-provider": "^1.7.7",
    "antd": "^5.4.5",
    "bignumber.js": "^9.0.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.2",
    "ethers": "^5.5.4",
    "graphql": "^16.2.0",
    "jotai": "^2.0.4",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "5.2.0",
    "util": "0.12.5",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@ethersproject/providers": "^5.6.8",
    "@iconify/json": "^2.2.55",
    "@loadable/component": "^5.15.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.27.2",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "depcheck": "^1.4.3",
    "detect-installer": "^1.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-i18n": "^2.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.0",
    "inquirer": "8.2.0",
    "jest-environment-node": "^28.1.3",
    "jsdom": "^21.1.1",
    "less": "^4.1.3",
    "lint-staged": "^10.0.0",
    "minimist": "^1.2.6",
    "mockjs": "^1.0.1-beta3",
    "mustache": "^4.2.0",
    "prettier": "^2.3.2",
    "prop-types": "^15.8.1",
    "puppeteer": "^16.1.0",
    "puppeteer-core": "^8.0.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^13.0.0",
    "unimported": "^1.21.0",
    "unocss": "^0.51.8",
    "vite": "^4.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}